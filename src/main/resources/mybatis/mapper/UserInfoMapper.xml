<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.travelplatform.web.mapper.UserInfoMapper">
    <insert id="insertInfo">
        INSERT INTO sqlconnect.travel_member(id,userName)
        VALUES (#{id}, #{userName});
    </insert>
    <select id="findInfoById" resultType="UserInfo">
        SELECT *
        FROM sqlconnect.travel_member
        WHERE id = #{id};
    </select>
</mapper>